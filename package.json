{
  "name": "indexless-query-benchmarks",
  "version": "0.1.0",
  "description": "Benchmark typical queries on Trino+Iceberg, PostgreSQL, and ClickHouse without indexes",
  "author": "Damir Manapov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/damir-manapov/indexless-query-benchmarks"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "generate": "tsx src/generate-data.ts",
    "generate:postgres": "tsx src/generate-data.ts --postgres",
    "generate:clickhouse": "tsx src/generate-data.ts --clickhouse",
    "generate:trino": "tsx src/generate-data.ts --trino",
    "benchmark": "tsx src/benchmark.ts",
    "benchmark:trino": "tsx src/benchmark.ts --trino",
    "benchmark:clickhouse": "tsx src/benchmark.ts --clickhouse",
    "benchmark:postgres": "tsx src/benchmark.ts --postgres",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "compose:up": "docker compose -f compose/docker-compose.yml up -d starter",
    "compose:up:postgres": "docker compose -f compose/docker-compose.yml up -d postgres",
    "compose:up:clickhouse": "docker compose -f compose/docker-compose.yml up -d clickhouse",
    "compose:up:trino": "docker compose -f compose/docker-compose.yml up -d trino",
    "compose:down": "docker compose -f compose/docker-compose.yml down",
    "compose:restart": "pnpm compose:down && pnpm compose:up",
    "compose:reset": "docker compose -f compose/docker-compose.yml down -v --remove-orphans"
  },
  "dependencies": {
    "@clickhouse/client": "^1.8.1",
    "@mkven/name-dictionaries": "^0.1.0",
    "@mkven/samples-generation": "0.4.1",
    "postgres": "^3.4.5",
    "trino-client": "^0.2.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^25.0.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-deprecation": "^3.0.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vitest": "^4.0.16"
  }
}
